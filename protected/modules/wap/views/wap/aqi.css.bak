	}
	.aqi_body .aqiframe_container{
		position: relative;
		left: 0;
		top: 0;
		width: 100%;
	}
	.aqi_body .aqiframe_container .frame{
		position: absolute;
		left: 100%;
		right: 0;
		width: 100%;
		display: none;
		padding-bottom: 60px;
		padding-top: 70px;
	}
	.aqi_body .aqiframe_container .frame.active{
		left: 0;
		display: block;
	}
	.aqi_body .promotion_container{
