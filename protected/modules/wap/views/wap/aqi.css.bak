	}
	#frame_order #order_list .control_group .less{
		display: inline-block;
		width: 35px;
		height: 35px;
		background: url('<?php echo Yii::app()->baseUrl?>/img/aqi/less.png') center center no-repeat; 
		background-size: cover;
